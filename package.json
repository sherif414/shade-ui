{
  "name": "shade-ui",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "up-dep": "taze major -I"
  },
  "dependencies": {
    "@floating-ui/vue": "^0.2.1",
    "@unocss/reset": "^0.49.4",
    "@vueuse/core": "^9.12.0",
    "motion": "^10.15.5",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.13.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.2.10",
    "@vue/tsconfig": "^0.1.3",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "taze": "^0.8.5",
    "typescript": "^4.9.5",
    "unocss": "^0.49.4",
    "unplugin-auto-import": "^0.14.2",
    "unplugin-vue-components": "^0.23.0",
    "vite": "^4.1.1",
    "vitest": "^0.28.4",
    "vue-tsc": "^1.0.24"
  }
}
