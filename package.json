{
  "name": "shade-ui",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.26.3",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "up-dep": "taze major -I",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@floating-ui/vue": "^0.2.1",
    "@vueuse/core": "^9.12.0",
    "motion": "^10.15.5",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.0.0-beta.50",
    "@storybook/addon-interactions": "7.0.0-beta.50",
    "@storybook/addon-links": "7.0.0-beta.50",
    "@storybook/blocks": "7.0.0-beta.50",
    "@storybook/testing-library": "0.0.14-next.1",
    "@storybook/vue3": "7.0.0-beta.50",
    "@storybook/vue3-vite": "7.0.0-beta.50",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.2.10",
    "@vue/tsconfig": "^0.1.3",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.0.0-beta.50",
    "taze": "^0.8.5",
    "typescript": "^4.9.5",
    "unocss": "^0.49.7",
    "vite": "^4.1.2",
    "vitest": "^0.28.5",
    "vue-tsc": "^1.1.3"
  }
}
