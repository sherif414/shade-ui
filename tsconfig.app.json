{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "playground/**/*.vue", "playground/**/*.ts"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "types": ["unplugin-vue-define-options/macros-global"],
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
